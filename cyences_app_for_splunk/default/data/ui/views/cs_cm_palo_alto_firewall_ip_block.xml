<form theme="dark" version="1.1" script="cs_cm_palo_ip_block.js">
  <label>Counter Measures - Palo Alto Firewall - IP Block/Allow</label>
  <fieldset submitButton="false">
    <input type="text" token="tkn_firewall_ip" searchWhenChanged="false">
      <label>Palo Alto Firewall IP</label>
    </input>
  </fieldset>
  <row>
    <panel>
      <input type="text" token="tkn_ip_address">
        <label>IP Address</label>
      </input>
      <html>
        <input type="button" class="btn btn-primary pull-left" id="btn_block_ip" value="Block" style="margin-left: 20px;"/>
        <input type="button" class="btn btn-primary pull-left" id="btn_unblock_ip" value="Unblock" style="margin-left: 20px;"/>
        <div id="palo_ip_block_add_modal_here"/>
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <html>
        <input type="button" class="btn btn-primary pull-left" id="btn_show_blocked_ip" value="Show Blocked IP" style="margin-left: 20px;"/>
        <div id="palo_blocked_ip_list_add_modal_here"/>
        <br/>
        <div id="palo_blocked_ip_list"/>
      </html>
    </panel>
  </row>
</form>