<dashboard version="1.1" theme="dark" script="cs_cyences_setup.js" stylesheet="cs_cyences_setup.css">
  <label>Cyences Setup</label>
  <description>Setup Cyences App</description>
  <row>
    <panel>
      <html>
        <form id="regForm" action="">
          <h1>Setup Cyences App</h1>
          <!-- One "tab" for each step in the form: -->
          <div class="tab">
            Select Products you user:
            <div id="productList">
            </div>
          </div>
          <div class="tab">
            Contact Info:
            <p>
              <input placeholder="E-mail..."></input>
            </p>
            <p>
              <input placeholder="Phone..."></input>
            </p>
          </div>
          <div class="tab">
            Birthday:
            <p>
              <input placeholder="dd"></input>
            </p>
            <p>
              <input placeholder="mm"></input>
            </p>
            <p>
              <input placeholder="yyyy"></input>
            </p>
          </div>
          <div class="tab">
            Login Info:
            <p>
              <input placeholder="Username..."></input>
            </p>
            <p>
              <input placeholder="Password..."></input>
            </p>
          </div>
          <div style="overflow:auto;">
            <div style="float:right;">
              <button type="button" id="prevBtn">Previous</button>
              <button type="button" id="nextBtn">Next</button>
            </div>
          </div>
          <!-- Circles which indicates the steps of the form: -->
          <div style="text-align:center;margin-top:40px;">
            <span class="step"></span>
            <span class="step"></span>
            <span class="step"></span>
            <span class="step"></span>
          </div>
        </form>
      </html>
    </panel>
  </row>
</dashboard>